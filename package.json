{
  "name": "@wynn719/vue-content-placeholders",
  "version": "0.0.1",
  "description": "Vue Components for rendering animated content placeholders like facebook",
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "plugin",
    "loader",
    "fake",
    "placeholder",
    "progressive",
    "skeleton-ui"
  ],
  "license": "MIT",
  "author": "Wynn",
  "repository": {
    "type": "git",
    "url": "https://github.com/wynn719/vue-content-placeholders.git"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "bugs": "https://github.com/wynn719/vue-content-placeholders/issues",
  "scripts": {
    "prepare": "husky install",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "dev": "vite --mode dev",
    "build": "pnpm clean:dist && vite build --mode prod && pnpm types",
    "types": "vue-tsc --emitDeclarationOnly --outDir ./dist/types",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "pnpm clean:coverage && vitest --coverage --mode test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.9",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "^0.12.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.9",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/runtime": "^7.18.9",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-legacy": "^2.0.1",
    "@vitejs/plugin-vue": "^2.2.0",
    "babel": "^6.23.0",
    "c8": "^7.11.0",
    "commitizen": "^4.2.4",
    "core-js": "^3.25.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.11.0",
    "happy-dom": "^2.49.0",
    "husky": "^7.0.4",
    "rimraf": "^3.0.2",
    "sass": "^1.54.5",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vitest": "^0.7.6",
    "vue": "^3.2.31",
    "vue-tsc": "^0.29.8"
  },
  "volta": {
    "node": "14.19.1"
  }
}
